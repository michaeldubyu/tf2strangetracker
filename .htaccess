RewriteEngine On

#RewriteCond %{REQUEST_URI} !^/index\.php
RewriteCond %{SCRIPT_FILENAME} !-d
RewriteCond %{SCRIPT_FILENAME} !-f

#RewriteRule ^userid/([^/\.]+)/^&item/([^/\.]+)/^track/([^/\.]+)/?$ index.php?page=$1&item=$2&track=$3 [L]
#RewriteRule ^userid/([^/\.]+)/^&item/([^/\.]+)/?$ index.php?usierid=$1&item=$2 [L]
RewriteRule ^index/(\w+)/?$ index.php?userid=$1 [L]

#RewriteRule ^p/(\w+)+/?$ index.php?p=$1 [L]
